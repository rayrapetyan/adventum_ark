  "faust_the_seven_games_of_the_soul": {
    "install": {
      "engine": "copy",
      "images": {
        "1.iso": {
          "{GAME_PATH}/data": [
            "DATA/eng/sy.at3",
            "DATA/*"
          ],
          "{GAME_PATH}": [
            "ames.ini",
            "aobj.ini",
            "arxrin3.fon",
            "faust.exe",
            "fl.ini",
            "mmxImage.dll"
          ]
        },
        "2.iso": {
          "{GAME_PATH}": [
            "./*"
          ]
        },
        "3.iso": {
          "{GAME_PATH}": [
            "./*"
          ]
        },
        "4.iso": {
          "{GAME_PATH}": [
            "./*"
          ]
        }
      },
      "patches": {
        "copy_file": {
          "{GAME_PATH}/data": [
            "cd.ini"
          ]
        },
        "find_replace_text": {
          "{GAME_PATH}/fl.ini": [
            {
              "find": "CDROM",
              "replace": ".\\"
            },
            {
              "find": "CHECKCD:[[:space:]]*1",
              "replace": "CHECKCD: 0"
            },
            {
              "find": "CHECKLOADSAVE:[[:space:]]*1",
              "replace": "CHECKLOADSAVE: 0"
            }
          ]
        }
      },
      "saves_path": "{GAME_PATH}/data/save"
    },
    "run": {
      "engine": "wine",
      "screen_resolution": "640x480",
      "exec_file": "faust.exe"
    }
  },
